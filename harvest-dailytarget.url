javascript:(function(){$(".timesheet-header .fl-right").prepend('Target: <input type="number" id="__daily_target" value="8.00" step="0.01" min="0.00" max="24.00" placeholder="8.00">');var l=$("#__daily_target");var e=function(){var e=Number(l.val()),t=$(".day-view-total"),a=Number(t.find("span").text()),n=Math.max(0,e-a),i=new Date((new Date).getTime()+n*36e5),r=i.getHours()+":"+(i.getMinutes()<10?"0":"")+i.getMinutes();t.html("("+n.toFixed(2)+" to "+r+") &nbsp; Total: <span>"+a.toFixed(2)+"</span>")};setInterval(e,2e3);l.on("input change",e)})()
